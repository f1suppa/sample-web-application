pipeline{
    agent any
    stages{
        stage("static code analysis"){
            agent {
                docker {
                    image 'maven'
                }
            }
            steps{
                script{
                    withSonarQubeEnv(credentialsId: 'sonar-token') {
                            sh "mvn sonar:sonar"      
                    }
                }         
            }
        }
    
    }
}